<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="SoukSearchPage" parent="GtkBox">
    <property name="hexpand">True</property>
    <property name="vexpand">True</property>
    <property name="orientation">vertical</property>
    <child>
      <object class="HdyHeaderBar">
        <property name="centering_policy">strict</property>
        <child>
          <object class="GtkButton">
            <property name="icon_name">go-previous-symbolic</property>
            <property name="action_name">win.go-back</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkSearchBar">
            <property name="search_mode_enabled">True</property>
            <child>
              <object class="HdyClamp">
                <property name="maximum_size">900</property>
                <property name="hexpand">True</property>
                <child>
                  <object class="GtkSearchEntry" id="search_entry">
                    <property name="margin_start">10</property>
                    <property name="margin_end">10</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkScrolledWindow" id="scrolled_window">
            <child>
              <object class="HdyClampScrollable">
                <property name="vexpand">True</property>
                <property name="maximum_size">900</property>
                <property name="hexpand">True</property>
                <child>
                  <object class="GtkListView" id="listview">
                    <property name="vexpand">true</property>
                    <property name="single_click_activate">True</property>
                    <style>
                      <class name="content"/>
                    </style>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>

